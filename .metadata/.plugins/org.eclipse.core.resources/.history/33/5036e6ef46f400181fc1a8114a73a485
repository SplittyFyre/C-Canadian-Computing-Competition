#include <stdio.h>
#include <memory.h>
#include <stdbool.h>

int n;
int temp;

int main(void) {

	puts("");

	scanf("%d", &n);
	int **mat = malloc(n * sizeof(int*));
	for (int i = 0; i < n; i++) {
		mat[i] = malloc(n * sizeof(int));
	}


	return 0;
}


