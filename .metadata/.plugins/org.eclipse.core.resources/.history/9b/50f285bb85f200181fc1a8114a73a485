#include <stdio.h>
#include <memory.h>
#include <stdbool.h>

int n;

bool validateColumns(int[n][n] )

int main(void) {

	puts("");

	scanf("%d", &n);
	int mat[n][n];
	memset(mat, 0, n * n * sizeof(int));

	int x;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			scanf("%d", &x);
			mat[i][j] = x;
		}
	}

	return 0;
}


