#include <stdio.h>
#include <stdlib.h>

int n;
int temp;

int main(void) {

	puts("");

	scanf("%d", &n);
	int **mat = malloc(n * sizeof(int*));
	for (int i = 0; i < n; i++) {
		mat[i] = malloc(n * sizeof(int));
	}

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			scanf("%d", &(mat[i][j]));
		}
	}

	for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				printf("%d\n", mat[i][j]);
			}
		}


	for (int i = 0; i < n; i++) {
		free(mat[i]);
	}
	free(mat);

	return 0;
}


