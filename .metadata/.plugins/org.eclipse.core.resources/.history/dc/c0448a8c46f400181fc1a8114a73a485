#include <stdio.h>
#include <memory.h>
#include <stdbool.h>

int n;
int temp;

bool validateColumns(int mat[n][n]) {
	for (int j = 0; j < n; j++) {
		for (int i = 1; i < n; i++) {
			if (mat[i - 1][j] >= mat[i][j]) {
				return false;
			}
		}
	}
	return true;
}

bool validateGrowing(int mat[n][n]) {
	for (int i = 0; i < n; i++) {
		for (int j = 1; j < n; j++) {
			if (mat[i][j - 1] >= mat[i][j]) {
				return false;
			}
		}
	}
	return true;
}

bool validate(int mat[n][n]) {
	return (validateColumns(mat) && validateGrowing(mat));
}



void transpose(int mat[n][n]) {
	for (int i = 0; i < n; i++) {
		for (int j = i; j < n; j++) {
			temp = mat[j][i];
			mat[j][i] = mat[i][j];
			mat[i][j] = temp;
		}
	}
}

void reverseColumnsOrRows(int mat[n][n]) {

}

int main(void) {

	puts("");

	scanf("%d", &n);
	int mat[n][n];
	memset(mat, 0, n * n * sizeof(int));

	int x;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			scanf("%d", &x);
			mat[i][j] = x;
		}
	}

	return 0;
}


